    function About(){
                var xhr =new XMLHttpRequest();
            xhr.onreadystatechange=function(){
                if(this.readyState==4&&this.status==200){
                    document.getElementById("res").innerHTML=this.responseText;
                }
                
                }
            
            xhr.open("GET","about.html" ,true);
            xhr.send();
        }
        </script>
